# enable mouse support
MOUSEKEY_ENABLE = yes
# enable audio and multimedia keys
EXTRAKEY_ENABLE = yes
# enable console for debugging
CONSOLE_ENABLE = no
# enable underglow
RGBLIGHT_ENABLE = yes
# enable knob
ENCODER_ENABLE = yes
# reduce size, increase build time
LTO_ENABLE = yes
# enable oled display
OLED_ENABLE = yes
# enable via support, only 4 layers applicable
VIA_ENABLED = no
# enable key override
KEY_OVERRIDE_ENABLE = no

